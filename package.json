{
  "name": "kissfft-wasm",
  "version": "1.2.3",
  "description": "A WebAssembly version of KissFFT.",
  "keywords": [
    "FFT",
    "WebAssembly",
    "KissFFT"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jest --maxWorkers=2",
    "lint": "eslint . --ext .ts,.js",
    "lint-fix": "eslint --fix . --ext .ts,.js",
    "build:wasm-debug": "cd ./deps && make debug && cd ..",
    "build:wasm": "cd ./deps && make release && cd ..",
    "build:jslib": "tsc -p . && cp src/kissfft.* lib/",
    "build-debug": "npm run build:wasm-debug && npm run build:jslib",
    "build": "npm run build:wasm && npm run build:jslib",
    "watch": "tsc -p . -w"
  },
  "author": "Vilja <i@vilja.me> (https://vilja.me)",
  "license": "MIT",
  "repository": "github:iVilja/kissfft-wasm",
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "jest": "^29.0.3",
    "ts-jest": "^29.0.2",
    "typescript": "^4.8.4"
  }
}
